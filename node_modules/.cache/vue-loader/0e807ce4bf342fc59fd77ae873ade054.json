s=\"['sportsNews','entertainment']\" ></b-form-select>\n  <button type=\"submit\" class=\"btn btn-primary\"  @click=\"submitHandler()\">Submit</button>\n</form>\n  </div></div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex'\n\nexport default {\n  name: 'addPost',\n  data () {\n    return {\n      id: Number,\n      comment: '',\n      postData: {\n        comments: [],\n        title: '',\n        des: '',\n        spec: null\n\n      }\n    }\n  },\n  methods: {\n    ...mapActions([\n      'addPost'\n    ]),\n    // in this form\n    submitHandler () {\n      const { id, title, des, spec } = this.postData\n      const payload = {\n        post: {\n          id,\n          title,\n          des\n        },\n        spec\n      }\n      this.addPost(payload)\n      this.postData = {\n        title: '',\n        des: '',\n        comments: [],\n        spec: null\n      }\n      this.sendDatabase(payload)\n      console.log(payload)\n    },\n    sendDatabase (payload) {\n      this.axios.post('http://localhost:8080/api/v1/add/', payload.post)\n        .then(response => {\n          this.postss = response.data\n          console.log(this.postss)\n        })\n    }\n  }\n}\n</script>\n"]}]}